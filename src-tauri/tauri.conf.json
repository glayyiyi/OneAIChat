{
  "$schema": "../node_modules/@tauri-apps/cli/schema.json",
  "identifier": "host.oneai.chat",
  "build": {
    "beforeBuildCommand": "yarn export",
    "beforeDevCommand": "yarn export:dev",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000"
  },
  "app": {
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 600,
        "resizable": true,
        "title": "OneAI Chat",
        "width": 960,
        "label": "main"
      }
    ]
  },
  "bundle": {
    "active": true,
    "category": "DeveloperTool",
    "copyright": "2023, One AI All Rights Reserved.",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "longDescription": "OneAI Chat is a cross-platform ChatGPT client, including Web/Win/Linux/OSX/PWA.",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13"
    },
    "iOS": {
      "developmentTeam": "auto"
    },
    "resources": [],
    "shortDescription": "OneAI Chat"
  },
  "productName": "oneai-chat",
  "version": "2.15.5"
}
